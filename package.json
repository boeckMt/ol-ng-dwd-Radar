    "@angular/language-service": "^19.0.5",
    "@types/jasmine": "^3.10.7",
    "@types/jasminewd2": "~2.0.10",
    "@types/luxon": "^3.2.0",
    "@types/node": "^15.14.9",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.3.20",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "protractor": "~7.0.0",
